{
	"name": "neliosoftware/nelio-maps",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"description": "Google Maps Block",
	"homepage": "https://neliosoftware.com/",
	"require-dev": {
		"automattic/vipwpcs": "^3.0.0",
		"composer/installers": "^2.2.0",
		"phpcompatibility/php-compatibility": "^9.3",
		"squizlabs/php_codesniffer": "^3.11.1",
		"wp-coding-standards/wpcs": "^3.1.0",
		"phpstan/phpstan": "^2.1",
		"szepeviktor/phpstan-wordpress": "^2.0",
		"phpstan/extension-installer": "^1.4"
	},
	"keywords": [
		"blocks",
		"editor",
		"gutenberg",
		"plugin",
		"wordpress"
	],
	"scripts": {
		"format": "phpcbf --standard=phpcs.ruleset.xml --report-summary --report-source --ignore=\"assets/dist/,node_modules/\"",
		"lint": "phpcs --standard=phpcs.ruleset.xml --ignore=\"assets/dist/,node_modules/\""
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
